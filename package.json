{
  "name": "doc-editor",
  "version": "2.3.0",
  "description": "Easily edit documents inside your opensearch indices",
  "opensearchDashboards": {
    "version": "2.3.0",
    "templateVersion": "2.3.0"
  },
  "scripts": {
    "osd": "node ../../scripts/osd",
    "es": "node ../../scripts/es",
    "build": "yarn plugin-helpers build",
    "plugin-helpers": "node ../../scripts/plugin_helpers",
    "lint": "../../node_modules/.bin/eslint .",
    "start": "node ../../scripts/plugin_helpers start --oss --no-watch",
    "test:server": "node ../../scripts/plugin_helpers test:server",
    "test:browser": "node ../../scripts/plugin_helpers test:browser"
  },
  "dependencies": {
    "react-fontawesome": "^1.7.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "typescript": "4.0.2",
    "gulp-rename": "2.0.0"
  }
}
