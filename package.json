{
  "name": "doc-editor",
  "version": "7.9.1",
  "description": "Easily edit documents inside your elasticsearch indices",
  "scripts": {
    "build": "rm -rf ./target && tsc",
    "preinstall": "node ../../preinstall_check",
    "kbn": "node ../../scripts/kbn",
    "es": "node ../../scripts/es",
    "test": "grunt test",
    "test:karma": "grunt test:karma",
    "test:karma:debug": "grunt test:karmaDebug",
    "test:jest": "node ../../scripts/jest",
    "test:jest_integration": "node ../../scripts/jest_integration",
    "test:mocha": "node ../../scripts/mocha",
    "test:mocha:coverage": "grunt test:mochaCoverage",
    "test:ftr": "node ../../scripts/functional_tests",
    "test:ftr:server": "node ../../scripts/functional_tests_server",
    "test:ftr:runner": "node ../../scripts/functional_test_runner",
    "test:coverage": "grunt test:coverage",
    "start": "node ../../scripts/kibana --dev",
    "debug": "node --nolazy --inspect ../../scripts/kibana --dev",
    "debug-break": "node --nolazy --inspect-brk ../../scripts/kibana --dev",
    "lint": "yarn run lint:es && yarn run lint:sass",
    "lint:es": "node ../../scripts/eslint",
    "lint:sass": "node ../../scripts/sasslint",
    "build:types": "tsc --p ../../tsconfig.types.json",
    "kbn:bootstrap": "node ../../scripts/register_git_hook"
  },
  "dependencies": {
    "react-fontawesome": "^1.7.1",
    "@elastic/eslint-config-kibana": "link:../../packages/eslint-config-kibana",
    "@elastic/eslint-import-resolver-kibana": "link:../../packages/kbn-eslint-import-resolver-kibana",
    "@kbn/expect": "link:../../packages/kbn-expect",
    "@kbn/plugin-helpers": "link:../../packages/kbn-plugin-helpers"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "tsc": "^1.20150623.0",
    "typescript": "3.9.5",
    "gulp-rename": "2.0.0"
  }
}
