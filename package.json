{
  "name": "doc-editor",
  "version": "7.4.0-1",
  "description": "Easily edit documents inside your elasticsearch indices",
  "main": "index.js",
  "kibana": {
    "version": "7.4.0",
    "templateVersion": "1.0.0"
  },
  "scripts": {
    "preinstall": "node ../../preinstall_check",
    "kbn": "node ../../scripts/kbn",
    "es": "node ../../scripts/es",
    "lint": "eslint .",
    "start": "plugin-helpers start",
    "test:server": "plugin-helpers test:server",
    "test:browser": "plugin-helpers test:browser",
    "test": "jest --verbose --color",
    "coverage": "jest --verbose --color --coverage --coverageReporters \"json\" --coverageReporters \"lcov\" --coverageReporters \"text\" --coverageReporters \"text-summary\" --coverageReporters \"clover\" --collectCoverageFrom \"(public|server)/**/*.js\" --coveragePathIgnorePatterns \"__(mocks|tests)__/\"",
    "build": "plugin-helpers build"
  },
  "dependencies": {
    "@elastic/eui": "^14.4.0",
    "react-fontawesome": "^1.6.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@elastic/elasticsearch": "^7.4.0",
    "@elastic/eslint-config-kibana": "link:../../packages/eslint-config-kibana",
    "@elastic/eslint-import-resolver-kibana": "link:../../packages/kbn-eslint-import-resolver-kibana",
    "@hapi/hapi": "^18.4.0",
    "@kbn/expect": "link:../../packages/kbn-expect",
    "@kbn/plugin-helpers": "link:../../packages/kbn-plugin-helpers",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-rewire": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.10.2"
  }
}
