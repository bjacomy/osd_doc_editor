{
  "name": "doc-editor",
  "version": "7.9.1",
  "description": "Easily edit documents inside your elasticsearch indices",
  "kibana": {
    "version": "kibana",
    "templateVersion": "1.0.0"
  },
  "scripts": {
    "preinstall": "node ../../preinstall_check",
    "kbn": "node ../../scripts/kbn",
    "es": "node ../../scripts/es",
    "lint": "../../node_modules/.bin/eslint .",
    "start": "../../node_modules/.bin/plugin-helpers start",
    "test:server": "../../node_modules/.bin/plugin-helpers test:server",
    "test:browser": "../../node_modules/.bin/plugin-helpers test:browser",
    "build": "../../node_modules/.bin/plugin-helpers build"
  },
  "dependencies": {
    "react-fontawesome": "^1.7.1",
    "@elastic/eslint-config-kibana": "link:../../packages/eslint-config-kibana",
    "@elastic/eslint-import-resolver-kibana": "link:../../packages/kbn-eslint-import-resolver-kibana",
    "@kbn/expect": "link:../../packages/kbn-expect",
    "@kbn/plugin-helpers": "link:../../packages/kbn-plugin-helpers"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "typescript": "3.9.5",
    "gulp-rename": "2.0.0"
  }
}
